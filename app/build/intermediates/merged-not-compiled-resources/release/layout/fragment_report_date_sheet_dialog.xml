<?xml version="1.0" encoding="utf-8"?>

                                                       
                                                   

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:background="@drawable/dialog_bg"
        android:orientation="vertical"
        android:paddingTop="8dp"
        android:paddingBottom="8dp" android:tag="layout/fragment_report_date_sheet_dialog_0" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/linear_close"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="10dp">

                <ImageView
                    android:id="@+id/close_cashier_float_amt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_close_black" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="70dp"
                    android:text="Select Date"
                    android:textColor="#1d1d1d"
                    android:textSize="20sp" />


            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="10dp">

            <!--<view xmlns:android="http://schemas.android.com/apk/res/android"-->
            <!--android:id="@+id/divider"-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="1dp"-->
            <!--android:background="#30111111" />-->

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#cccccc" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/Lay1"
            android:layout_width="325dp"
            android:layout_height="50dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/linear_product_circle3"
            android:visibility="visible">

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:textColorHint="@color/dark_blue">

                <EditText
                    android:id="@+id/edit_text_datecount"
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/linear_product_circle"
                    android:hint="DATE RANGE"
                    android:inputType="textCapCharacters"
                    android:textAllCaps="true"
                    android:textColor="@color/mine_shaft"
                    android:textColorHint="@color/dark_blue"
                    android:textSize="14sp" />
            </com.google.android.material.textfield.TextInputLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/LayStarting"
            android:layout_width="325dp"
            android:layout_height="50dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/linear_product_circle3"
            android:visibility="visible">

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:textColorHint="@color/dark_blue">

                <EditText
                    android:id="@+id/edit_text_starting"
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/linear_product_circle"
                    android:hint="STARTING"
                    android:inputType="textCapCharacters"
                    android:textAllCaps="true"
                    android:textColor="@color/mine_shaft"
                    android:textColorHint="@color/dark_blue"
                    android:textSize="14sp" />

            </com.google.android.material.textfield.TextInputLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp">

                <ImageView
                    android:id="@+id/img_text_starting"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/ic_arrow_down_grey_500" />
            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:id="@+id/LayEnding"
            android:layout_width="325dp"
            android:layout_height="50dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/linear_product_circle3"
            android:visibility="visible">

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:textColorHint="@color/dark_blue">

                <EditText
                    android:id="@+id/edit_text_ending"
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/linear_product_circle"
                    android:hint="ENDING"
                    android:inputType="textCapCharacters"
                    android:textAllCaps="true"
                    android:textColor="@color/mine_shaft"
                    android:textColorHint="@color/dark_blue"
                    android:textSize="14sp" />

            </com.google.android.material.textfield.TextInputLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp">

                <ImageView
                    android:id="@+id/img_text_ending"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/ic_arrow_down_grey_500" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/LayPrevious"
            android:layout_width="325dp"
            android:layout_height="50dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/linear_product_circle3"
            android:visibility="visible">

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:textColorHint="@color/dark_blue">

                <EditText
                    android:id="@+id/type_name"
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/linear_product_circle"
                    android:hint="Previous Report"
                    android:inputType="textCapCharacters"
                    android:textAllCaps="true"
                    android:textColor="@color/mine_shaft"
                    android:textColorHint="@color/dark_blue"
                    android:textSize="14sp" />

            </com.google.android.material.textfield.TextInputLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp">

                <ImageView
                    android:id="@+id/btn_type"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/ic_arrow_down_grey_500" />
            </LinearLayout>
        </LinearLayout>

